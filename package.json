{
  "name": "@reown/appkit-monorepo",
  "version": "1.7.1",
  "private": true,
  "type": "module",
  "author": "Reown <support@reown.com> (https://reown.com)",
  "license": "Apache-2.0",
  "homepage": "https://github.com/reown-com/appkit",
  "bugs": {
    "url": "https://github.com/reown-com/appkit/issues"
  },
  "scripts": {
    "prebuild": "node scripts/inject-version.js",
    "build:clean": "turbo run build:clean --filter={./packages/*}",
    "build:sample-apps": "turbo run build --filter={./examples/*}",
    "build:example:wagmi-cdn": "bun run build; bun --filter @examples/html-wagmi-cdn build",
    "build:example:vue-wagmi": "bun --filter @examples/vue-wagmi build",
    "build:example:parcel-react-wagmi": "bun --filter @examples/parcel-react-wagmi build",
    "build:example:wagmi-app-router": "bun --filter @examples/next-wagmi-app-router build",
    "build": "turbo run build --filter={./packages/**/*} --concurrency=31",
    "watch": "turbo run watch --filter={./packages/*,./packages/adapters/*} --concurrency=50 --continue",
    "gallery": "turbo run dev --filter={./apps/gallery}",
    "gallery-new": "turbo run dev --filter={./apps/gallery-new}",
    "laboratory": "bun --filter @apps/laboratory dev",
    "builder:dev": "bun --filter @apps/demo dev",
    "demo:dev": "bun --filter @apps/demo dev",
    "builder:build": "bun run build; bun --filter @apps/demo build",
    "demo:build": "bun run build; bun --filter @apps/demo build",
    "laboratory-new": "bun --filter @apps/laboratory-new dev",
    "browser-extension": "bun --filter @apps/browser-extension dev",
    "examples": "turbo run dev --filter={./examples/*}",
    "build:gallery": "bun run build; bun --filter=./apps/gallery build",
    "build:gallery-new": "bun run build; bun --filter=./apps/gallery-new build",
    "build:laboratory": "bun run build; bun --filter=./apps/laboratory build",
    "build:demo": "bun --filter=./apps/demo build",
    "build:laboratory-new": "bun run build; bun --filter=./apps/laboratory-new build",
    "build:examples": "bun run build; bun --filter=./examples -r build",
    "build:browser-extension": "bun --filter @apps/browser-extension build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage.enabled=true --coverage.reporter=html --coverage.reportOnFailure=true --coverage.all=false --coverage.reportsDirectory=.coverage && vite preview --outDir .coverage --open --port 4777",
    "test:ci": "vitest run --coverage.enabled=true --coverage.reporter=json --coverage.reporter=json-summary --coverage.reportOnFailure=true --coverage.all=false",
    "test:static": "pnpm run typecheck; pnpm run lint; pnpm run prettier",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "danger": "danger ci",
    "prettier": "prettier --check .",
    "prettier:format": "prettier --write .",
    "clean": "rm -rf `find . -type d -name node_modules -o -name dist -o -name .next -o -name out`; rm pnpm-lock.yaml; rm bun.lock",
    "changeset:pre": "changeset pre enter",
    "changeset:pre:exit": "changeset pre exit",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "bun run publish:latest",
    "publish:latest": "bun run install; bun run build; changeset publish",
    "publish:alpha": "bun run install; bun run build; changeset publish --tag alpha",
    "publish:beta": "bun run install; bun run build; changeset publish --tag beta",
    "publish:canary": "bun run install; bun run build; changeset publish --tag canary",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/assemble-release-plan": "6.0.6",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.28.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@vitest/coverage-v8": "2.1.9",
    "@vitest/ui": "2.1.8",
    "buffer": "5.5.0||6.0.0",
    "danger": "12.3.4",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-require-extensions": "0.1.3",
    "husky": "9.1.7",
    "jsdom": "24.1.0",
    "pino": "7.11.0",
    "prettier": "3.5.3",
    "process": "0.11.10",
    "turbo": "2.0.6-canary.0",
    "typescript": "5.7.3",
    "vite": "5.4.12",
    "vite-plugin-node-polyfills": "0.23.0",
    "vitest": "2.1.9"
  },
  "packageManager": "pnpm@9.5.0",
  "pnpm": {
    "overrides": {
      "@solana/web3.js": "1.98.0",
      "elliptic": ">=6.6.1",
      "axios": ">=1.8.2",
      "ws": ">=7.5.10"
    },
    "patchedDependencies": {
      "@changesets/assemble-release-plan@6.0.6": "patches/@changesets__assemble-release-plan@6.0.6.patch"
    }
  },
  "overrides": {
    "@solana/web3.js": "1.98.0",
    "elliptic": ">=6.6.1",
    "axios": ">=1.8.2",
    "ws": ">=7.5.10"
  },
  "resolutions": {
    "@solana/web3.js": "1.98.0",
    "elliptic": ">=6.6.1",
    "axios": ">=1.8.2",
    "ws": ">=7.5.10"
  },
  "patchedDependencies": {
    "@changesets/assemble-release-plan@6.0.6": "patches/@changesets__assemble-release-plan@6.0.6.patch"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "workspaces": [
    "packages/adapters/*",
    "packages/appkit",
    "packages/common",
    "packages/ui",
    "packages/controllers",
    "packages/core-legacy",
    "packages/appkit-utils",
    "packages/scaffold-ui",
    "packages/polyfills",
    "packages/wallet",
    "packages/siwe",
    "packages/siwx",
    "packages/wallet-button",
    "packages/cdn",
    "packages/experimental",
    "packages/cli",
    "apps/*",
    "examples/*",
    "services/*",
    "packages/ui-new",
    "!packages/appkit-new",
    "!apps/gallery-new",
    "!apps/laboratory-new"
  ]
}
