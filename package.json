{
  "name": "@reown/appkit-monorepo",
  "version": "1.7.1",
  "private": true,
  "type": "module",
  "author": "Reown (https://discord.gg/reown)",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://github.com/reown-com/appkit",
  "bugs": {
    "url": "https://github.com/reown-com/appkit/issues"
  },
  "scripts": {
    "prebuild": "node scripts/inject-version.js",
    "prebuild:all": "node scripts/inject-version.js",
    "build:clean": "turbo run build:clean --filter={./packages/*}",
    "build:sample-apps": "TURBO_UI=0 turbo run build --filter={./examples/*}",
    "build:example:wagmi-cdn": "pnpm build; pnpm --filter @examples/html-wagmi-cdn build",
    "build:example:vue-wagmi": "pnpm --filter @examples/vue-wagmi build",
    "build:example:parcel-react-wagmi": "pnpm --filter @examples/parcel-react-wagmi build",
    "build:example:wagmi-app-router": "pnpm --filter @examples/next-wagmi-app-router build",
    "build": "turbo run build --filter={./packages/**/*} --filter=!{./packages/cdn} --concurrency=31",
    "build:all": "turbo run build --filter={./packages/**/*} --concurrency=31",
    "watch": "turbo run watch --filter={./packages/*,./packages/adapters/*} --concurrency=50 --continue",
    "gallery": "turbo run dev --filter={./apps/gallery}",
    "laboratory": "pnpm --filter @apps/laboratory dev",
    "builder:dev": "pnpm --filter @apps/demo dev",
    "demo:dev": "pnpm --filter @apps/demo dev",
    "builder:build": "pnpm build; pnpm --filter @apps/demo build",
    "demo:build": "pnpm build; pnpm --filter @apps/demo build",
    "browser-extension": "pnpm --filter @apps/browser-extension dev",
    "examples": "turbo run dev --filter={./examples/*}",
    "build:gallery": "pnpm build; pnpm --filter=./apps/gallery build",
    "build:laboratory": "pnpm build; pnpm --filter=./apps/laboratory build",
    "build:demo": "pnpm --filter=./apps/demo build",
    "build:examples": "pnpm build; pnpm --filter=./examples -r build",
    "build:browser-extension": "pnpm --filter @apps/browser-extension build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage.enabled=true --coverage.reporter=html --coverage.reportOnFailure=true --coverage.all=false --coverage.reportsDirectory=.coverage && vite preview --outDir .coverage --open --port 4777",
    "test:ci": "vitest run --coverage.enabled=true --coverage.reporter=json --coverage.reporter=json-summary --coverage.reportOnFailure=true --coverage.all=false",
    "test:static": "pnpm run typecheck; pnpm run lint; pnpm run prettier",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "danger": "danger ci",
    "prettier": "prettier --check .",
    "prettier:format": "prettier --write .",
    "clean": "rm -rf `find . -type d -name node_modules -o -name dist -o -name .next -o -name out`; rm pnpm-lock.yaml",
    "changeset:pre": "changeset pre enter",
    "changeset:pre:exit": "changeset pre exit",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "pnpm publish:latest",
    "publish:latest": "pnpm install; pnpm build:all; changeset publish",
    "publish:alpha": "pnpm install; pnpm build:all; changeset publish --tag alpha",
    "publish:beta": "pnpm install; pnpm build:all; changeset publish --tag beta",
    "publish:canary": "pnpm install; pnpm build:all; changeset publish --tag canary",
    "prepare": "husky",
    "cli": "node packages/cli/index.js",
    "codemod:upgrade": "node packages/codemod/src/index.js upgrade $*",
    "codemod:upgrade:wc": "node packages/codemod/src/index.js upgrade @walletconnect/* && pnpm install",
    "codemod:upgrade:wagmi": "node packages/codemod/src/index.js upgrade viem wagmi @wagmi/* && pnpm install",
    "codemod:upgrade:solana": "node packages/codemod/src/index.js upgrade @solana/* && pnpm install",
    "codemod:upgrade:radix": "node packages/codemod/src/index.js upgrade @radix-ui/* && pnpm install",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "size:json": "size-limit --json"
  },
  "devDependencies": {
    "@changesets/assemble-release-plan": "6.0.6",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.28.1",
    "@size-limit/file": "11.1.6",
    "@size-limit/preset-big-lib": "11.1.6",
    "@size-limit/webpack": "11.1.6",
    "@trivago/prettier-plugin-sort-imports": "5.2.1",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@vitest/coverage-v8": "2.1.9",
    "@vitest/ui": "2.1.8",
    "buffer": "5.5.0||6.0.0",
    "chalk": "4.1.2",
    "danger": "13.0.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-require-extensions": "0.1.3",
    "events": "3.1.0",
    "husky": "9.1.7",
    "jsdom": "24.1.0",
    "pino": "7.11.0",
    "prettier": "3.5.3",
    "process": "0.11.10",
    "size-limit": "11.1.6",
    "stream-browserify": "3.0.0",
    "turbo": "2.5.8",
    "typescript": "5.8.3",
    "vite": "5.4.20",
    "vite-plugin-node-polyfills": "0.23.0",
    "vitest": "2.1.9"
  },
  "packageManager": "pnpm@9.5.0",
  "pnpm": {
    "overrides": {
      "@solana/web3.js": "1.98.4",
      "esbuild": ">=0.25.1",
      "@babel/runtime": ">=7.26.10",
      "cookie": ">=0.7.0",
      "elliptic": ">=6.6.1",
      "ws": ">=7.5.10",
      "chalk": "4.1.2",
      "strip-ansi": "6.0.1",
      "color-convert": "<=2.0.1",
      "color-name": "<=1.1.4",
      "is-core-module": "<=2.13.1",
      "error-ex": "<=1.3.2",
      "has-ansi": "<=5.0.1",
      "oxc-parser": "0.56.5"
    },
    "patchedDependencies": {
      "@changesets/assemble-release-plan@6.0.6": "patches/@changesets__assemble-release-plan@6.0.6.patch"
    }
  },
  "patchedDependencies": {
    "@changesets/assemble-release-plan@6.0.6": "patches/@changesets__assemble-release-plan@6.0.6.patch"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
