  "type": "module",
  "main": "./dist/esm/exports/index.js",
  "types": "./dist/types/exports/index.d.ts",
  "files": [
    "dist",
    "!tsconfig.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/exports/index.d.ts",
      "import": "./dist/esm/exports/index.js",
      "default": "./dist/esm/exports/index.js"
    },
    "./w3m-modal": {
      "types": "./dist/types/exports/w3m-modal.d.ts",
