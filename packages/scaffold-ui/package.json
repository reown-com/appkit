      "default": "./dist/esm/exports/embedded-wallet.js"
    },
    "./socials": {
      "types": "./dist/types/exports/socials.d.ts",
      "import": "./dist/esm/exports/socials.js",
      "default": "./dist/esm/exports/socials.js"
