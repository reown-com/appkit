    "@tanstack/react-query": "5.24.8",
    "@web3modal/ethers": "4.2.2",
    "@web3modal/siwe": "4.2.2",
    "@web3modal/solana": "4.2.2",
    "@web3modal/wagmi": "4.2.2",
